{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://openweathermap.org/root.json",
  "type": "object",
  "title": "root",
  "required": [
    "coord",
    "weather",
    "base",
    "main",
    "visibility",
    "wind",
    "clouds",
    "dt",
    "sys",
    "id",
    "name",
    "cod"
  ],
  "properties": {
    "coord": {
      "$id": "#/properties/coord",
      "type": "object",
      "title": "coord",
      "required": [
        "lon",
        "lat"
      ],
      "properties": {
        "lon": {
          "$id": "#/properties/coord/properties/lon",
          "type": "number",
          "title": "lon"
        },
        "lat": {
          "$id": "#/properties/coord/properties/lat",
          "type": "number",
          "title": "lat"
        }
      }
    },
    "weather": {
      "$id": "#/properties/weather",
      "type": "array",
      "title": "weather",
      "items": {
        "$id": "#/properties/weather/items",
        "type": "object",
        "title": "items",
        "required": [
          "id",
          "main",
          "description",
          "icon"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/weather/items/properties/id",
            "type": "number",
            "title": "id"
          },
          "main": {
            "$id": "#/properties/weather/items/properties/main",
            "type": "string",
            "title": "main"
          },
          "description": {
            "$id": "#/properties/weather/items/properties/description",
            "type": "string",
            "title": "description"
          },
          "icon": {
            "$id": "#/properties/weather/items/properties/icon",
            "type": "string",
            "title": "icon"
          }
        }
      }
    },
    "base": {
      "$id": "#/properties/base",
      "type": "string",
      "title": "base"
    },
    "main": {
      "$id": "#/properties/main",
      "type": "object",
      "title": "main",
      "required": [
        "temp",
        "pressure",
        "humidity",
        "temp_min",
        "temp_max"
      ],
      "properties": {
        "temp": {
          "$id": "#/properties/main/properties/temp",
          "type": "number",
          "title": "temp"
        },
        "pressure": {
          "$id": "#/properties/main/properties/pressure",
          "type": "number",
          "title": "The Pressure Schema"
        },
        "humidity": {
          "$id": "#/properties/main/properties/humidity",
          "type": "number",
          "title": "humidity"
        },
        "temp_min": {
          "$id": "#/properties/main/properties/temp_min",
          "type": "number",
          "title": "temp_min"
        },
        "temp_max": {
          "$id": "#/properties/main/properties/temp_max",
          "type": "number",
          "title": "temp_max"
        }
      }
    },
    "visibility": {
      "$id": "#/properties/visibility",
      "type": "number",
      "title": "visibility"
    },
    "wind": {
      "$id": "#/properties/wind",
      "type": "object",
      "title": "wind",
      "required": [
        "speed"
      ],
      "properties": {
        "speed": {
          "$id": "#/properties/wind/properties/speed",
          "type": "number",
          "title": "speed"
        },
        "deg": {
          "$id": "#/properties/wind/properties/deg",
          "type": "number",
          "title": "deg"
        }
      }
    },
    "clouds": {
      "$id": "#/properties/clouds",
      "type": "object",
      "title": "clouds",
      "required": [
        "all"
      ],
      "properties": {
        "all": {
          "$id": "#/properties/clouds/properties/all",
          "type": "number",
          "title": "all"
        }
      }
    },
    "dt": {
      "$id": "#/properties/dt",
      "type": "number",
      "title": "dt"
    },

    "sys": {
      "$id": "#/properties/sys",
      "type": "object",
      "title": "sys",
      "required": [
        "type",
        "id",
        "message",
        "country",
        "sunrise",
        "sunset"
      ],
      "properties": {
        "type": {
          "$id": "#/properties/sys/properties/type",
          "type": "number",
          "title": "type"
        },

        "id": {
          "$id": "#/properties/sys/properties/id",
          "type": "number",
          "title": "id"
        },

        "message": {
          "$id": "#/properties/sys/properties/message",
          "type": "number",
          "title": "message"
        },

        "country": {
          "$id": "#/properties/sys/properties/country",
          "type": "string",
          "title": "country"
        },

        "sunrise": {
          "$id": "#/properties/sys/properties/sunrise",
          "type": "number",
          "title": "sunrise"
        },

        "sunset": {
          "$id": "#/properties/sys/properties/sunset",
          "type": "number",
          "title": "sunset"
        }

      }
    },

    "id": {
      "$id": "#/properties/id",
      "type": "number",
      "title": "id"
    },

    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "name"
    },

    "cod": {
      "$id": "#/properties/cod",
      "type": "number",
      "title": "name"
    }

  }
}